{
  "root": "apps/authentication",
  "sourceRoot": "apps/authentication/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/authentication",
        "main": "apps/authentication/src/main.ts",
        "tsConfig": "apps/authentication/tsconfig.app.json",
        "assets": ["apps/authentication/src/assets"],
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/authentication/src/environments/environment.ts",
              "with": "apps/authentication/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "build-image": {
      "builder": "@nrwl/workspace:run-commands",
      "options": {
        "command": "docker build -f ./apps/authentication/dev.Dockerfile . -t {args.dockerid}/authentication:dev-{args.dockertag}"
      },
      "configurations": {
        "production": {
          "command": "docker build -f ./apps/authentication/Dockerfile . -t {args.dockerid}/authentication:{args.dockertag}"
        }
      }
    },
    "deploy": {
      "builder": "@nrwl/workspace:run-commands",
      "options": {
        "command": "docker push {args.dockerid}/authentication:dev-{args.dockertag}"
      },
      "configurations": {
        "production": {
          "command": "docker push {args.dockerid}/authentication:{args.dockertag}"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "authentication:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/authentication/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/authentication"],
      "options": {
        "jestConfig": "apps/authentication/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
